{
  "name": "agent-starter-node",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint -f unix \"**/*.ts\"",
    "format": "prettier --write \"**/*.ts\"",
    "lint:fix": "eslint --fix \"**/*.ts\"",
    "download-files": "npm run build && node dist/agent.js download-files",
    "dev": "npm run build && node dist/agent.js dev",
    "start": "npm run build && node dist/agent.js start"
  },
  "packageManager": "pnpm@9.15.9",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.16.5",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@livekit/agents": "^0.7.8",
    "@livekit/agents-plugin-cartesia": "^0.1.4",
    "@livekit/agents-plugin-deepgram": "^0.5.7",
    "@livekit/agents-plugin-openai": "^0.9.3",
    "@livekit/agents-plugin-silero": "^0.5.8",
    "@livekit/rtc-node": "^0.13.18",
    "dotenv": "^16.6.1",
    "zod": "^3.25.76"
  }
}
